{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "title": "COVID-19 Active Cases (10 Oct 2020)",
  "width": 800,
  "height": 600,
  "projection": {"type": "equirectangular"},
  "data": {
    "url": "https://raw.githubusercontent.com/Exteriors/FIT3179Assignment3/main/Assignment3/obeserate.json",
    "format": {"type": "topojson", "feature": "obeserate"}
  },
  "transform": [
    {
      "lookup": "properties.name",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/Exteriors/FIT3179Assignment3/main/Assignment3/Fat_Supply_Quantity_Data.csv"
        },
        "key": "Country",
        "fields": ["Obesity"]
      }
    },
    {"calculate": "datum.Obesity", "as": "Obesity Rate"}
  ],
  "mark": {"type": "geoshape"},
  "encoding": {
    "color": {
      "field": "Obesity Rate",
      "type": "quantitative",
      "scale": {
        "type": "threshold",
        "domain": [10, 25, 35],
        "range": ["#fdbe85", "#fd8d3c", "#e6550d", "#a63603"]
      }
    },
    "tooltip": [
      {"field": "properties.name", "type": "nominal", "title": "Country"},
      {"field": "Obesity Rate", "type": "quantitative", "format": ".2f"}
    ]
  },
  "config": {}
}
